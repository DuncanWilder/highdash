{
  "name": "highdash",
  "version": "0.0.1",
  "description": "A lightweight replacement for Lodash",
  "main": "dist/index.js",
  "scripts": {
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --fix ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --fix ./src",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "build": "rm -rf dist && tsc -p tsconfig-build.json && node ./file-size.js",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanOgle/highdash.git"
  },
  "author": "Duncan Ogle",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/DuncanOgle/highdash/issues"
  },
  "homepage": "https://github.com/DuncanOgle/highdash#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/lodash": "4.17.16",
    "@vitest/coverage-istanbul": "3.0.7",
    "chalk": "4.1.2",
    "globals": "16.0.0",
    "lodash": "4.17.21",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "vitest": "3.0.7"
  }
}
