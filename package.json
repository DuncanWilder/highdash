  },
  "homepage": "https://github.com/DuncanWilder/highdash#readme",
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@types/lodash": "4.17.20",
    "@vitest/coverage-istanbul": "4.0.12",
    "chalk": "4.1.2",
    "globals": "16.5.0",
    "lodash": "4.17.21",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "4.0.12"
  }
